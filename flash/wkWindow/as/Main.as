// == START PROGRAM =====================================================================function startProgram(){	trace("- Start program.");	// start the timer that calls the onTick function    refreshTimer.start();	}var firstTime:Boolean = true;var counter1:Number = 0;var counter2:Number = 0;var counter3:Number = 0;var counter4:Number = 0;function onTick(event:TimerEvent):void {		var analogValue1:Number;	var analogValue2:Number;	var analogValue3:Number;	var analogValue4:Number;			analogValue1 = a.getAnalogData(0);	analogValue2 = a.getAnalogData(1);	analogValue3 = a.getAnalogData(2);	analogValue4 = a.getAnalogData(3);			if(analogValue1>10) {				counter1++;		trace("counter =" + counter1);				trace("knock1");		gotoAndPlay("bird");			}	if(analogValue2>10) {		counter2++;		trace("counter =" + counter2);				trace("knock2");		gotoAndPlay("cat");			}	if(analogValue3>10) {				counter3++;		trace("counter =" + counter3);				trace("knock3");		gotoAndPlay("dog");			}	if(analogValue4>10) {				counter4++;		trace("counter =" + counter4);				trace("knock4");		gotoAndPlay("fish");			}}	